{{#sprite}}
{{class}}-sprites(@selector: ~'') {
    &{{class}} @{selector} {
        background-image: url('{{{escaped_image}}}');
    }
{{/sprite}}
{{#items}}
    &{{class}} @{selector} {
        background-position: {{px.offset_x}} {{px.offset_y}};
        width: {{px.width}};
        height: {{px.height}};
    }
{{/items}}
}
